{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://www.google.com/search?*"
        }
      ],
      "deny": []
    },
    "fs:default",
    {
      "identifier": "fs:scope",
      "allow": [
        "$HOME/**",
        "$DOWNLOADS/**",
        "$DOCUMENTS/**",
        "$DESKTOP/**"
      ]
    },
    "fs:allow-write-file",
    "dialog:default"
  ]
}